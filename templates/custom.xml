<templateSet group="custom">
  <template name="builder" value="public $BUILDERCLASS$ $ATTRIBUTE$($ATTRIBUTETYPE$ $ATTRIBUTE$) {&#10;    this.$ATTRIBUTE$ = $ATTRIBUTE$;&#10;    return this;&#10;}&#10;$END$" description="" toReformat="false" toShortenFQNames="true">
    <variable name="BUILDERCLASS" expression="className(sClassName)" defaultValue="" alwaysStopAt="false" />
    <variable name="ATTRIBUTE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ATTRIBUTETYPE" expression="" defaultValue="String" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="fo" value="//format:off&#10;$SELECTION$&#10;//format:on" description="Disable format" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_STRING" value="true" />
    </context>
  </template>
</templateSet>